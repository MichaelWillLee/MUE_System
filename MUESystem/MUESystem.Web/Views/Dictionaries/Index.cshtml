@*@model IEnumerable<MUESystem.Model.Dictionary>*@
@model PagedList.IPagedList<MUESystem.Model.Dictionary>
@using PagedList.Mvc
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(modelTitle => modelTitle.Status)*@状态
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Reark)*@备注
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.FirstCode)*@一级纬度
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.SecondCode)*@二级纬度
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.ThirdCode)*@三级纬度
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.FourthCode)*@四级纬度
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.FifthCode)*@五级纬度
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.InfoValue)*@信息值
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Reark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ThirdCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FourthCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FifthCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InfoValue)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
  @*每页 @Model.PageSize 条记录，*@
共 @Model.PageCount 页，当前第 @Model.PageNumber 页
@Html.PagedListPager(Model, page => Url.Action("Index", new { page }))